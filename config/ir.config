{
	"mutate_columns":
	{
		"mutations":
		[
			{
				"in": "gwas_file",
				"out": "gwas_display_name",
				"map":
				{
					"BMI_GIANT_2018_txt_gz": "BMI",
					"FastGlu_MAGIC_Europeans_txt_gz": "Fasting-Glucose",
					"FastInsu_adjBMI_MAGIC_Europeans_txt_gz": "FastingInsulin-BMI-Adjusted",
					"HDL_GLGC_Expanded_txt_gz" : "High-Density-Lipoprotein",
					"TG_GLGC_Expanded_txt_gz": "Triglycerides",
					"MI_adjBMI_European_txt_gz": "M-I",
					"MAGIC_ISI_Model_2_AgeSexBMI_txt_txt_gz": "Insulin-Sensitivity-Index-Model-2",
					"T2D_Mahajan_Europeans_txt_gz": "Type-2-Diabetes-Mahajan",
					"Type-2-Diabetes_Xue_2018.txt.gz": "Type-2-Diabetes-Xue",
					"Type-2-Diabetes_Suzuki_2019.txt.gz": "Type-2-Diabetes-Suzuki",
					"Type-2-Diabetes_Spracklen_2020.txt.gz": "Type-2-Diabetes-Spracklen",
					"WHR-adj-BMI_GIANT_2018_txt_gz": "Waist-Hip-Ratio-BMI-Adjusted"	
				}
			},
			{
				"in": "gwas_file",
				"out": "gwas_short_trait",
				"map":
				{
					"BMI_GIANT_2018_txt_gz": "BMI",
					"FastGlu_MAGIC_Europeans_txt_gz": "FG",
					"FastInsu_adjBMI_MAGIC_Europeans_txt_gz": "FI",
					"HDL_GLGC_Expanded_txt_gz" : "HDL",
					"TG_GLGC_Expanded_txt_gz": "TG",
					"MI_adjBMI_European_txt_gz": "MI",
					"MAGIC_ISI_Model_2_AgeSexBMI_txt_txt_gz": "ISI",
					"T2D_Mahajan_Europeans_txt_gz": "T2D",
					"Type-2-Diabetes_Xue_2018.txt.gz": "T2D",
					"Type-2-Diabetes_Suzuki_2019.txt.gz": "T2D",
					"Type-2-Diabetes_Spracklen_2020.txt.gz": "T2D",
					"WHR-adj-BMI_GIANT_2018_txt_gz": "WHR"	
				}
			},

			{
				"in": "qtl_file",
				"out": "qtl_display_name",
				"map":
				{
					"Liver_allpairs_txt_gz_eQTLs_txt_gz": "Liver-eQTL",
					"Adipose_Visceral_Omentum_allpairs_txt_gz_eQTLs_txt_gz": "Visceral-Adipose-eQTL",
					"Adipose_Subcutaneous_allpairs_txt_gz_eQTLs_txt_gz": "Subcutaneous-Adipose-eQTL",
					"Muscle_Skeletal_allpairs_txt_gz_eQTLs_txt_gz": "Skeletal-Muscle-eQTL",
					"Pancreas_allpairs_txt_gz_eQTLs_txt_gz": "Pancreas-eQTL",
					"Liver_sQTLs_txt_gz": "Liver-sQTL",
					"Adipose_Visceral_Omentum_sQTLs_txt_gz": "Visceral-Adipose-sQTL",
					"Adipose_Subcutaneous_sQTLs_txt_gz": "Subcutaneous-Adipose-sQTL",
					"Muscle_Skeletal_sQTLs_txt_gz": "Skeletal-Muscle-sQTL",
					"Pancreas_sQTLs_txt_gz": "Pancreas-sQTL"
				}
			},
			{
				"in": "qtl_file",
				"out": "qtl_tissue",
				"map":
				{
					"Liver_allpairs_txt_gz_eQTLs_txt_gz": "Liver",
					"Adipose_Visceral_Omentum_allpairs_txt_gz_eQTLs_txt_gz": "Visceral-Adipose",
					"Adipose_Subcutaneous_allpairs_txt_gz_eQTLs_txt_gz": "Subcutaneous-Adipose",
					"Muscle_Skeletal_allpairs_txt_gz_eQTLs_txt_gz": "Skeletal-Muscle",
					"Pancreas_allpairs_txt_gz_eQTLs_txt_gz": "Pancreas",
					"Liver_sQTLs_txt_gz": "Liver",
					"Adipose_Visceral_Omentum_sQTLs_txt_gz": "Visceral-Adipose",
					"Adipose_Subcutaneous_sQTLs_txt_gz": "Subcutaneous-Adipose",
					"Muscle_Skeletal_sQTLs_txt_gz": "Skeletal-Muscle",
					"Pancreas_sQTLs_txt_gz": "Pancreas"
				}
			}

		]
	},

	"post_hoc_filter":
	{
		"kept_gwas":
		[
			"BMI_GIANT_2018_txt_gz",
			"FastGlu_MAGIC_Europeans_txt_gz",
			"FastInsu_adjBMI_MAGIC_Europeans_txt_gz",
			"HDL_GLGC_Expanded_txt_gz",
			"TG_GLGC_Expanded_txt_gz",
			"MI_adjBMI_European_txt_gz",
			"MAGIC_ISI_Model_2_AgeSexBMI_txt_txt_gz",
			"T2D_Mahajan_Europeans_txt_gz",
			"Type-2-Diabetes_Xue_2018.txt.gz",
			"Type-2-Diabetes_Suzuki_2019.txt.gz",
			"Type-2-Diabetes_Spracklen_2020.txt.gz",
			"WHR-adj-BMI_GIANT_2018_txt_gz"
		],

		"kept_qtl":
		[
			"Liver_allpairs_txt_gz_eQTLs_txt_gz",
			"Adipose_Visceral_Omentum_allpairs_txt_gz_eQTLs_txt_gz",
			"Adipose_Subcutaneous_allpairs_txt_gz_eQTLs_txt_gz",
			"Muscle_Skeletal_allpairs_txt_gz_eQTLs_txt_gz",
			"Pancreas_allpairs_txt_gz_eQTLs_txt_gz",
			"Liver_sQTLs_txt_gz",
			"Adipose_Visceral_Omentum_sQTLs_txt_gz",
			"Adipose_Subcutaneous_sQTLs_txt_gz",
			"Muscle_Skeletal_sQTLs_txt_gz",
			"Pancreas_sQTLs_txt_gz"	
		],
		"gwas_pval_threshold":			
		{
			"standard": 5e-8,		
			"exceptions":			
			{
				"MI_adjBMI_European_txt_gz": 1e-5,
				"MAGIC_ISI_Model_2_AgeSexBMI_txt_txt_gz": 1e-5
			}
		},
		"qtl_pval_threshold":			
		{
			"standard": 1e-5
		},
		"snp_count_min_threshold": 20
	},
	"add_hgnc_names":
	{
		"ensemble_to_hgnc_map_file": "data/hgnc/ensembl_to_hgnc.txt",
		"ensembl_col_index": 1,
		"hgnc_col_index": 3
	},
	"add_rsids":
	{
		"genome_file": "hg38"
	},
	"assign_locus_numbers":
	{
		"genome_file": "hg38"
	},

	"classify_results":
	{
		"colocalization_threshold_score": 0.35,
		
		"rules" :
		{
			"step1_coloc_sorting":
			{
				"type": "num_colocs",
				"categories":
				{
					"one_candidate_one_coloc":
					{
						"num_candidates_equals": "1",
						"num_colocs_equals": "1"
					},
					"one_candidate_no_coloc":
					{
						"num_candidates_equals": "1",
						"num_colocs_equals": "0"
					},
					"multi_candidate_one_coloc":
					{
						"num_candidates_greater_than": "1",
						"num_colocs_equals": "1"
					},
					"multi_candidate_multi_coloc":
					{
						"num_candidates_greater_than": "1",
						"num_colocs_greater_than": "1"
					},
					"multi_candidate_no_coloc":
					{
						"num_candidates_greater_than": "1",
						"num_colocs_equals": "0"
					}
				}
			},
			"step2_tissue_sorting":
			{
				"type": "specificity",
				"column": "qtl_tissue",
				"categories":
				[
					{
						"class_name": "AdpS",
						"contains_exactly":
						[
							"Subcutaneous-Adipose"
						]
					},
					{
						"class_name": "AdpV",
						"contains_exactly":
						[
							"Visceral-Adipose"
						]
					},
					{
						"class_name": "AdpS+V",
						"contains_exactly":
						[
							"Subcutaneous-Adipose",
							"Visceral-Adipose"
						]
					},
					{
						"class_name": "Musk",
						"contains_exactly":
						[
							"Skeletal-Muscle"
						]
					},
					{
						"class_name": "Liver",
						"contains_exactly":
						[
							"Liver"
						]
					},
					{
						"class_name": "Pancreas",
						"contains_exactly":
						[
							"Pancreas"
						]
					},
					{
						"class_name": "Other"
					}
				]
			},
			"step3_gwas_sorting":
			{
				"type": "specificity",
				"column": "gwas_short_trait",
				"categories":
				[
					{
						"class_name": "IR",
						"contains_some":
						[
							"MI",
							"ISI"
						]
					},
					{
						"class_name": "T2D-FG-FI",
						"contains_some":
						[
							"T2D",
							"FG",
							"FI"
						]
					},
					{
						"class_name": "WHR",
						"contains_some":
						[
							"WHR"
						]
					},
					{
						"class_name": "TG-HDL",
						"contains_some":
						[
							"TG",
							"HDL"
						]
					},
					{
						"class_name": "BMI",
						"contains_some":
						[
							"BMI"
						]
					}
				]
			}
		}
	},
	"get_ld_buddies":
	{
		"ld_file": "data/ld/EUR_geno_hg38.txt.gz",
		"genome": "hg38"
	}
}
